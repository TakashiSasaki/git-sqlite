# git-sqlite v0.0.1 by Takashi SASAKI https://twitter.com/TakashiSasaki
# Add these filter definition to your .git/config file.
# Do not forget to modify .gitattributes file in your repository as well.

[filter "sqlite2"]
  smudge = sqlite2restore
  clean = sqlite2dump

[filter "sqlite3"]
  smudge = sqlite3restore
  clean = sqlite3dump

